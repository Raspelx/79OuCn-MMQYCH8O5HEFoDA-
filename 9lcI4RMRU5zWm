local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Key System", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

_G.Key = "yamete"
_G.KeyInput = "string"

function MakeScriptHub()
    if game.PlaceId == 7288936208 then -- checks the game id
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Raspelx/79OuCn-MMQYCH8O5HEFoDA-/main/91hJoGZQJMxxoGmYT6tJTg%3D%3D"))()
    print("Success")
    else
    print("Game Not Supported") -- change the text to whatever
    end
    
    if game.PlaceId == 13059253052 then -- checks the game id
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Raspelx/79OuCn-MMQYCH8O5HEFoDA-/main/wC1RFEcMdXc%2BfAOsxEwFkA%3D%3D"))()
    print("Success")
    else
    print("Game Not Supported") -- change the text to whatever
    end
    
    if game.PlaceId == 7922878642 then -- checks the game id
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Raspelx/79OuCn-MMQYCH8O5HEFoDA-/main/ibLiIt1o3KgCMHKc4x7JZw%3D%3D"))()
    print("Success")
    else
    print("Game Not Supported") -- change the text to whatever
    end
    
    if game.PlaceId == 5117512593 then -- checks the game id
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Raspelx/79OuCn-MMQYCH8O5HEFoDA-/main/ix4WULRgz453iJ0g3oa"))()
    print("Success")
    else
    print("Game Not Supported") -- change the text to whatever
    end    
    OrionLib:Destroy()
end

function CorrectKeyNotifaction()
    OrionLib:MakeNotification({
        Name = "Correct Key!",
        Content = "You have entered the correct key!",
        Image = "rbxassetid://4483345998",
        Time = 5
    }) 
end

function IncorrectKeyNotifaction()
    OrionLib:MakeNotification({
        Name = "Incorrect Key :p",
        Content = "You have entered the incorrect key!",
        Image = "rbxassetid://4483345998",
        Time = 5
    }) 
end

local Tab = Window:MakeTab({
	Name = "Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "Check Key",
	Callback = function()
      		if _G.KeyInput == _G.Key then
            MakeScriptHub()
            CorrectKeyNotifaction()
            else
                IncorrectKeyNotifaction()
        end
  	end    
})
OrionLib:Init()
