local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Key System", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

_G.Key = "yamete"
_G.KeyInput = "string"

function MakeScriptHub()
    local gameid = game.PlaceId
if(gameid == 7288936208) then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Raspelx/astrax/main/sbs"))()
else
print("game not supported")

local gameid = game.PlaceId
if(gameid == 13059253052) then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Raspelx/astrax/main/eca%20training"))()
else
print("game not supported")

local gameid = game.PlaceId
if(gameid == 7922878642) then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Raspelx/astrax/main/sbs%20training"))()
else
print("game not supported")

local gameid = game.PlaceId
if(gameid == 5117512593) then
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Raspelx/astrax/main/soccer%20legaue"))()
else
print("game not supported")
    OrionLib:Destroy()
end

function CorrectKeyNotifaction()
    OrionLib:MakeNotification({
        Name = "Correct Key!",
        Content = "You have entered the correct key!",
        Image = "rbxassetid://4483345998",
        Time = 5
    }) 
end

function IncorrectKeyNotifaction()
    OrionLib:MakeNotification({
        Name = "Incorrect Key :p",
        Content = "You have entered the incorrect key!",
        Image = "rbxassetid://4483345998",
        Time = 5
    }) 
end

local Tab = Window:MakeTab({
	Name = "Key",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddTextbox({
	Name = "Enter Key",
	Default = "",
	TextDisappear = true,
	Callback = function(Value)
		_G.KeyInput = Value
	end	  
})

Tab:AddButton({
	Name = "Check Key",
	Callback = function()
      		if _G.KeyInput == _G.Key then
            MakeScriptHub()
            CorrectKeyNotifaction()
            else
                IncorrectKeyNotifaction()
        end
  	end    
})
OrionLib:Init()
